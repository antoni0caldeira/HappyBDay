                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.IdDepartmentsNavigation.Name)
                </dd>
            </dl>
        </div>
        <div>
            <a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-primary">Edit</a>
            <a onclick="history.go(-1)" class="btn btn-light">Back</a>
        </div>
        </div>
</section>